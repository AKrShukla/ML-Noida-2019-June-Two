    "import numpy as np\n",
    "import matplotlib.pyplot as plt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "entropy_list = []\n",
    "for red in range(1, 50):\n",
    "    black = 50 - red\n",
    "    \n",
    "    entro = - (red/50 * np.log2(red/50) + black/50 * np.log2(black/50))\n",
    "    entropy_list.append(entro)"
   ]
  },
